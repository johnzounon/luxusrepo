{% extends 'base.html.twig' %}

{% block title %}ProductCondition{% endblock %}

{% block body %}
    <h1>ProductCondition</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ product_condition.id }}</td>
            </tr>
            <tr>
                <th>Title_fr</th>
                <td>{{ product_condition.titleFr }}</td>
            </tr>
            <tr>
                <th>Title_de</th>
                <td>{{ product_condition.titleDe }}</td>
            </tr>
            <tr>
                <th>Title_it</th>
                <td>{{ product_condition.titleIt }}</td>
